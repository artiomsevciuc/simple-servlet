<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>HelloWorld</servlet-name>
		<servlet-class>md.utm.simpleservlet.examle.HelloServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>CreatePerson</servlet-name>
		<servlet-class>md.utm.simpleservlet.examle.CreatePersonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadPersonServlet</servlet-name>
		<servlet-class>md.utm.simpleservlet.examle.ReadPersonServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloWorld</servlet-name>
		<url-pattern>/hello</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CreatePerson</servlet-name>
		<url-pattern>/createPerson</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadPersonServlet</servlet-name>
		<url-pattern>/readPerson</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
